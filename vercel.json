{
    "version": 2,
    "builds": [
        {
            "src": "my_site/wsgi.py",
            "use": "vercel-python@3.9",
            "config": {
                "maxLambdaSize": "15mb"
            }
        }
    ],
    "routes": [
        {
            "src": "/static/(.*)",
            "dest": "/static/$1"
        },
        {
            "src": "/(.*)",
            "dest": "my_site/wsgi.py"
        }
    ]
}
