{
    "builds": [
        {
            "src": "my_site/wsgi.py",
            "use": "@vercel/python",
            "config": {
                "runtime": "python3.8",
                "maxLambdaSize": "15mb"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "my_site/wsgi.py"
        }
    ]
}
